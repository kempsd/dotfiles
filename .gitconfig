[user]
	name = Dorian Kemps
	email = kemps.dorian@gmail.com
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = auto
[core]
	editor = nano
	excludesfile = ~/.gitignore_global
[push]
	default = current
[alias]
        clo = clone
        co  = checkout
        st  = status
        pur = pull --rebase
        ci  = commit
        rc  = rebase --continue
        lg  = log -M --decorate --graph --oneline
        br  = branch
        sth = stash
